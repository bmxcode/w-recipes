                        difficulty="2"
                        fileReference="/content/dam/w-recipes/recipes/images/spicy-vodka-rigatoni-v3_Desktop-1300x658.jpeg"
                        id="spicy-vodka-rigatoni"
                        method="&lt;h3>Step 1&lt;/h3>&#xd;&#xa;&lt;p>Cook the rigatoni according to the packet instructions or until al dente.&lt;/p>&#xd;&#xa;&lt;h3>Step 2&lt;/h3>&#xd;&#xa;&lt;p>Meanwhile, heat the olive oil in a large frying pan over medium heat. Add the chilli flakes and stir for around 30 seconds until the oil turns red.&lt;/p>&#xd;&#xa;&lt;h3>Step 3&lt;/h3>&#xd;&#xa;&lt;p>Add the vodka, stir for a few seconds, then add the jar of Mutti Gourmet Pasta Sauce Rossoro Tomatoes With Genovese Basil, and stir to combine. Finally, add the cream and warm together for about 5 minutes.&lt;/p>&#xd;&#xa;&lt;h3>Step 4&lt;/h3>&#xd;&#xa;&lt;p>Drain the pasta and add it to the sauce, tossing to combine. Add most of the parmesan to the pan and toss to combine.&lt;/p>&#xd;&#xa;&lt;h3>Step 5&lt;/h3>&#xd;&#xa;&lt;p>Top with remaining parmesan and serve immediately.&lt;/p>&#xd;&#xa;"
                        prep="5 min"
                        serves="6"
                        textIsRich="true">
                        <ingredients jcr:primaryType="nt:unstructured">
                            <item0
                                jcr:primaryType="nt:unstructured"
                                ingredient="1 jar Mutti Gourmet Pasta Sauce Rossoro Tomatoes With Genovese Basil"/>
                            <item1
                                jcr:primaryType="nt:unstructured"
                                ingredient="500g dried rigatoni"/>
                            <item2
                                jcr:primaryType="nt:unstructured"
                                ingredient="1 tbs olive oil"/>
                            <item3
                                jcr:primaryType="nt:unstructured"
                                ingredient="1 tbs chilli flakes"/>
                            <item4
                                jcr:primaryType="nt:unstructured"
                                ingredient="60ml vodka"/>
                            <item5
                                jcr:primaryType="nt:unstructured"
                                ingredient="300ml cream"/>
                            <item6
                                jcr:primaryType="nt:unstructured"
                                ingredient="200g parmesan, finely grated"/>
                        </ingredients>
